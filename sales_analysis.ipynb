# Import libraries
import pandas as pd
import matplotlib.pyplot as plt

# Load dataset
df = pd.read_csv("sales_data.csv")

# Explore data
print(df.head())
print(df.describe())

# Sales by region
sales_by_region = df.groupby('region')['sales_amount'].sum()
sales_by_region.plot(kind='bar', title='Total Sales by Region')
plt.show()
